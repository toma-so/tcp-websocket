{
  "name": "@toma.com/tcp-websocket",
  "description": "A WebSocket client-only class made with TCP streams.",
  "version": "0.1.5",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": false,
  "exports": {
    ".": {
      "bun": "./dist/index.js",
      "node": "./dist/index.js",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun x rimraf ./dist ./tsconfig.tsbuildinfo && bun run build:types && bun run build:bun",
    "build:bun": "bun build ./src/index.ts --target bun --sourcemap=external --format esm --outdir ./dist --minify",
    "build:types": "bun x tsc"
  },
  "license": "MIT",
  "author": "Toma.com Team (Originally Mikkel RINGAUD <mikkel@milescode.dev>)",
  "repository": {
    "url": "https://github.com/toma-so/tcp-websocket",
    "type": "git"
  },
  "bugs": "https://github.com/toma-so/tcp-websocket/issues",
  "keywords": [
    "bun",
    "tcp",
    "websocket"
  ],
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.0.0"
  }
}
